<%= simple_form_for @camp do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :location %>
  <%= f.input :price %>
  <%= f.input :capacity %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.button :submit %>
<% end %>
